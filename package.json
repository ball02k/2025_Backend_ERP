{
  "name": "2025_backend_erp",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon index.cjs",
    "dev:all": "bash scripts/dev-all.sh",
    "db:up": "docker compose up -d db",
    "db:down": "docker compose down",
    "db:logs": "docker compose logs -f db",
    "seed": "node prisma/seed.cjs",
    "dev-seed": "node scripts/dev-seed.cjs",
    "seed:demo": "node scripts/dev-seed.cjs",
    "seed:reset": "prisma migrate reset --force && node scripts/dev-seed.cjs",
    "postinstall": "prisma generate",
    "prisma:validate": "prisma validate",
    "prisma:generate": "prisma generate",
    "prisma:deploy": "prisma migrate deploy",
    "db:migrate": "prisma migrate deploy",
    "health": "node -e \"fetch('http://localhost:'+ (process.env.PORT||3001) +'/health').then(r=>{if(!r.ok)process.exit(1);return r.json()}).then(x=>console.log(x.ok?'ok':'fail')).catch(()=>process.exit(1))\"",
    "smoke:docs": "node scripts/documents-smoke.mjs",
    "smoke:rfis": "node scripts/smoke_rfis.mjs",
    "smoke:qa": "node scripts/smoke_qa.mjs",
    "smoke:hs": "node scripts/smoke_hs.mjs",
    "smoke:carbon": "node scripts/smoke_carbon.mjs",
    "smoke:newmodules": "npm run smoke:rfis && npm run smoke:qa && npm run smoke:hs && npm run smoke:carbon",
    "smoke:variations": "node scripts/variations-smoke.mjs",
    "test": "jest --runInBand",
    "api:inventory": "node scripts/api_inventory.js",
    "api:catalog": "node scripts/api_inventory.js --write-md",
    "api:openapi": "node scripts/api_inventory.js --write-openapi"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.617.0",
    "@aws-sdk/s3-request-presigner": "^3.617.0",
    "@prisma/client": "6.14.0",
    "cors": "^2.8.5",
    "express": "4.19.2",
    "jsonwebtoken": "9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "prisma": "6.14.0",
    "jest": "^29.7.0",
    "supertest": "^7.0.0"
  },
  "engines": {
    "node": "20.16.x"
  },
  "prisma": { "seed": "node prisma/seed.cjs" }
}
